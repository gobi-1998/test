#Public route table code

resource "aws_route_table" "public-rt1" {
  vpc_id = aws_vpc.luffy-vpc.id

  route {
    cidr_block = "0.0.0.0/0"
    gateway_id = aws_internet_gateway.test-igw1.id
  }


  tags = {
    Name = "public-rt1"
  }
}
