  }
}

# Create a public IP for NAT gateway
resource "aws_eip" "naruto_nat-eip" {}

# Create a NAT gateway
resource "aws_nat_gateway" "my-ngw1" {
  allocation_id = aws_eip.naruto_nat-eip.id
  subnet_id     = aws_subnet.public-subnet1.id
}

# Create private route table
resource "aws_route_table" "pri-rt1" {
  vpc_id = aws_vpc.luffy-vpc.id

  route {
    cidr_block = "0.0.0.0/0"
    gateway_id = aws_nat_gateway.my-ngw1.id
  }

  tags = {
    Name = "pri-rt1"
  }
}

# Route table association for private subnet
resource "aws_route_table_association" "private-asso1" {
  subnet_id      = aws_subnet.private-subnet1.id
  route_table_id = aws_route_table.pri-rt1.id
}

